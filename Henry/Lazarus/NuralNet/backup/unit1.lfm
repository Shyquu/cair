  object M_Info: TMemo
    Left = 0
    Height = 474
    Top = 0
    Width = 474
    Lines.Strings = (
      'Dies ist ein kleines Programm zu Visualisiren von Neuralen Netzen.'
      ''
      'Um ein Model zu lden Drücken sie die Lade Taste und wählen sie die .json Datei aus.'
      'Bei Mehreren Modellen in einer Datei wird der Speicher vorher Gelöscht.'
      'Einzelne Modelle werden in die Liste mit hinzugefügt.'
      ''
      'Mit Zeichnen wir des im moment ausgewählte Model gezeigt.'
      'Mit Nächstes wir das Nächste Modell ausgewählt.'
      ''
      'Der Auto Toggel durchläuft, wen eingeschaltet, automatisch jedes Modell nacheinander.'
      'Der Regler darunter bestimmt die Geschwindigkeit des Durchwächselns.'
    )
